{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1",
            "headers": {
                "Content-Type": "application/javascript"
            }
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1",
            "headers": {
                "Content-Type": "text/css"
            }
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1",
            "headers": {
                "Content-Type": "image/png"
            }
        },
        {
            "src": "/style.css",
            "dest": "/public/css/style.css",
            "headers": {
                "Content-Type": "text/css"
            }
        },
        {
            "src": "/favicon-(.*)",
            "dest": "/public/favicon-$1",
            "headers": {
                "Content-Type": "image/png"
            }
        },
        {
            "src": "/(.*)\\.png",
            "dest": "/public/images/$1.png",
            "headers": {
                "Content-Type": "image/png"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}