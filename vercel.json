{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/js/(.*)", "dest": "/public/js/$1" },
      { "src": "/css/(.*)", "dest": "/public/css/$1" },
      { "src": "/style.css", "dest": "/public/style.css" },
      { "src": "/(.*)\\.png", "dest": "/public/$1.png" },
      { "src": "/(.*)", "dest": "/index.js" }
    ]
  }
  